Algoritmo "Loops enquanto"

Var
   contador, final_contagem: inteiro


Inicio
   contador <- 0
   Escreval ("Digite um Número")
   Leia(final_contagem)
   Enquanto (contador <= final_contagem) faca
      Escreval("Está em ", contador)
      contador <- contador + 1
   FimEnquanto
   Escreval ("Finalmente chegou no ", contador)
Fimalgoritmo